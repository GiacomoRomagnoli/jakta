turn(self) : aligned([cell(_, _, x), cell(_, _, x), cell(_, _, x)]) <- .print("I won!");
turn(self) : aligned([cell(_, _, o), cell(_, _, o), cell(_, _, o)]) <- .print("I lost!");

turn(self) : aligned([cell(X, Y, e), cell(_, _, x), cell(_, _, x)]) <- put(X, Y, x).
turn(self) : aligned([cell(_, _, x), cell(X, Y, e), cell(_, _, x)]) <- put(X, Y, x).
turn(self) : aligned([cell(_, _, x), cell(_, _, x), cell(X, Y, e)]) <- put(X, Y, x).

turn(self) : aligned([cell(X, Y, e), cell(_, _, x), cell(_, _, e)]) <- put(X, Y, x).
turn(self) : aligned([cell(_, _, x), cell(X, Y, e), cell(_, _, e)]) <- put(X, Y, x).
turn(self) : aligned([cell(_, _, e), cell(X, Y, e), cell(_, _, x)]) <- put(X, Y, x).
turn(self) : aligned([cell(_, _, x), cell(_, _, e), cell(X, Y, e)]) <- put(X, Y, x).

turn(self) : cell(X, Y, e) <- put(X, Y, x).